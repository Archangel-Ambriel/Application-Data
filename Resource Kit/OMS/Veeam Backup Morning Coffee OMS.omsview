{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Veeam Morning Coffee Dashboard for Backup",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Veeam Morning Coffee Dashboard for Backup')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Veeam Morning Coffee Dashboard for Backup",
                        "Name": "Veeam Morning Coffee Dashboard for Backup",
                        "Author": "www.veeam.com",
                        "Source": "Local",
                        "Dashboard": [
						{
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "",
                                        "NewGroup": false,
                                         "Color": "#007233"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Morning Coffee Dashboard for Backup",
                                        "Link": {
                                            "Label": "More info",
                                            "Url": "http://vee.am/mpoms80"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "Overview",
                                            "Content": "This View is based on data collected by Veeam Management Pack for System Center. You can get more details about Veeam Management Pack [here](http://vee.am/oms/mpoverview \"here\")"
                                        },
                                        {
                                            "Title": "Troubleshooting",
                                            "Content": "If you don't see any data check the following:\n\n    1. Operations Manager Server with Veeam MP\n    is connected as source to the OMS Workspace\n    2. Veeam Integration Pack for OMS has been\n    imported in Operations Manager\n    3. You have a valid license for Veeam MP\n    and Backup servers are enabled for monitoring\n    \nYou can also check the [Troubleshooting section](http://vee.am/mpoms80/backup \"Troubleshooting section\") of the MP for Veeam Backup & Replication User Guide"
                                        }
                                    ]
                                }
                            },
                          {
                            "Id": "SingleQueryDonutBuilderBladeV1",
                            "Type": "Blade",
                            "Version": 0,
                            "Configuration": {
                              "General": {
                                "title": "Veeam Backup Servers",
                                "newGroup": false,
                                "icon": "",
                                "useIcon": false
                              },
                              "Header": {
                                "Title": "Current Health State",
                                "Subtitle": ""
                              },
                              "Donut": {
                                "Query": "Type=Perf CounterName=HealthState ObjectName=\"Veeam Backup Server\" InstanceName=_Total* TimeGenerated:[NOW-15minutes..NOW] | measure max(CounterValue) by InstanceName",
                                "CenterLegend": {
                                  "Text": "Total",
                                  "Operation": "Sum",
                                  "ArcsToSelect": [ ]
                                },
                                "Options": {
                                  "colors": [
                                    "#eb3c00",
                                    "#009e49",
                                    "#fff100"
                                  ],
                                  "valueColorMapping": [
                                    {
                                      "value": "_Total(Error)",
                                      "color": "#eb3c00"
                                    },
                                    {
                                      "value": "_Total(Warning)",
                                      "color": "#fff100"
                                    },
                                    {
                                      "value": "_Total(Success)",
                                      "color": "#009e49"
                                    },
                                    {
                                                    "value": "_Total(Uninitialized)",
                                                    "color": "#e2e584"
                                                },
									{
                                      "value": "_Total(MaintenanceMode)",
                                      "color": "#0072c6"
                                    },
									{
                                      "value": "_Total(UnAvailable)",
                                      "color": "#68217a"
                                    }
                                  ]
                                }
                              },
                              "List": {
                                "Query": "Type=Perf CounterName=Alerts ObjectName=\"Veeam Backup Server\" InstanceName<>_Total* TimeGenerated:[NOW-15minutes..NOW] | measure max(CounterValue) by InstanceName",
                                "HideGraph": false,
                                "enableSparklines": false,
                                "ColumnsTitle": {
                                  "Name": "VBR Server Name",
                                  "Value": "Active Alerts"
                                },
                                "Color": "#0072c6",
                                "operation": "Last Sample",
                                "thresholds": {
                                  "isEnabled": false,
                                  "values": [
                                    {
                                      "name": "Normal",
                                      "threshold": "Default",
                                      "color": "#eb3c00",
                                      "isDefault": true
                                    },
                                    {
                                      "name": "Warning",
                                      "threshold": "60",
                                      "color": "#eb3c00",
                                      "isDefault": false
                                    },
                                    {
                                      "name": "Error",
                                      "threshold": "90",
                                      "color": "#eb3c00",
                                      "isDefault": false
                                    }
                                  ]
                                },
                                "NameDSVSeparator": "",
                                "NavigationQuery": "Type:Alert AlertState!=Closed AlertName=Veeam*Backup* SourceDisplayName In {Type:Perf {selected item} | measure count() by InstanceName}"
                              }
                            }
